local hwidtable = loadstring(game:HttpGet('https://glot.io/snippets/gjluxzlvzv/raw/main.lua'))();
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
local ban = false
local whitelist = false
for i,aaa in next,hwidtable do
	if aaa[1] == HWID then
		if aaa[4] == true then
			ban = true
		end
		if aaa[3] == true then
			ban = false
			whitelist = true
		end
	end
end
wait()

